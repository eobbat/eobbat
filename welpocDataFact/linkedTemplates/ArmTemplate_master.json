{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "welpocDataFact"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "'AzureBlobStorage1' の 'connectionString' のセキュリティで保護された文字列"
		},
		"AzureFileStorage1_connectionString": {
			"type": "secureString",
			"metadata": "'AzureFileStorage1' の 'connectionString' のセキュリティで保護された文字列"
		},
		"AzurePostgreSql1_connectionString": {
			"type": "secureString",
			"metadata": "'AzurePostgreSql1' の 'connectionString' のセキュリティで保護された文字列"
		},
		"LNKCSV_WEL_TEISU_M_connectionString": {
			"type": "secureString",
			"metadata": "'LNKCSV_WEL_TEISU_M' の 'connectionString' のセキュリティで保護された文字列"
		},
		"LNKDB_TENPO_EXEC_SERVER_M_connectionString": {
			"type": "secureString",
			"metadata": "'LNKDB_TENPO_EXEC_SERVER_M' の 'connectionString' のセキュリティで保護された文字列"
		},
		"LNKDB_WEL_TEISU_M_connectionString": {
			"type": "secureString",
			"metadata": "'LNKDB_WEL_TEISU_M' の 'connectionString' のセキュリティで保護された文字列"
		},
		"containerUri": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを含むストレージ アカウントの URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを格納するストレージ アカウントへの SAS トークン"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "welpocDataFact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureFileStorage1_connectionString": {
						"value": "[parameters('AzureFileStorage1_connectionString')]"
					},
					"AzurePostgreSql1_connectionString": {
						"value": "[parameters('AzurePostgreSql1_connectionString')]"
					},
					"LNKCSV_WEL_TEISU_M_connectionString": {
						"value": "[parameters('LNKCSV_WEL_TEISU_M_connectionString')]"
					},
					"LNKDB_TENPO_EXEC_SERVER_M_connectionString": {
						"value": "[parameters('LNKDB_TENPO_EXEC_SERVER_M_connectionString')]"
					},
					"LNKDB_WEL_TEISU_M_connectionString": {
						"value": "[parameters('LNKDB_WEL_TEISU_M_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}