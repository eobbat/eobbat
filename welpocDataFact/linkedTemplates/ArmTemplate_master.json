{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "welpocDataFact"
		},
		"LNK_BLOB2_connectionString": {
			"type": "secureString",
			"metadata": "'LNK_BLOB2' の 'connectionString' のセキュリティで保護された文字列"
		},
		"LNK_DB2_connectionString": {
			"type": "secureString",
			"metadata": "'LNK_DB2' の 'connectionString' のセキュリティで保護された文字列"
		},
		"blob_point_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/29c9909d-38ec-4542-9320-c3871b022d56/resourceGroups/welpocgroup/providers/Microsoft.Storage/storageAccounts/welpocst"
		},
		"blob_point_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"postgresql_point_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/29c9909d-38ec-4542-9320-c3871b022d56/resourceGroups/welpocgroup/providers/Microsoft.DBforPostgreSQL/servers/welpocdb4"
		},
		"postgresql_point_properties_groupId": {
			"type": "string",
			"defaultValue": "postgresqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを含むストレージ アカウントの URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを格納するストレージ アカウントへの SAS トークン"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "welpocDataFact_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LNK_BLOB2_connectionString": {
						"value": "[parameters('LNK_BLOB2_connectionString')]"
					},
					"LNK_DB2_connectionString": {
						"value": "[parameters('LNK_DB2_connectionString')]"
					},
					"blob_point_properties_privateLinkResourceId": {
						"value": "[parameters('blob_point_properties_privateLinkResourceId')]"
					},
					"blob_point_properties_groupId": {
						"value": "[parameters('blob_point_properties_groupId')]"
					},
					"postgresql_point_properties_privateLinkResourceId": {
						"value": "[parameters('postgresql_point_properties_privateLinkResourceId')]"
					},
					"postgresql_point_properties_groupId": {
						"value": "[parameters('postgresql_point_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}